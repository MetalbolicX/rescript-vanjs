{
  "name": "rescript-vanjs",
  "description": "Rescript bindings for Van.js, a lightweight JavaScript framework for building web applications.",
  "version": "2.1.0",
  "scripts": {
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript -w",
    "build": "bun build ./example/Main.res.mjs --outdir ./dist --format esm",
    "dev": "vite",
    "res:test": "retest --with-dom test/**/*.res.mjs"
  },
  "keywords": [
    "rescript",
    "vanjs",
    "vanjs-core"
  ],
  "author": {
    "name": "José Martínez Santana"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetalbolicX/rescript-vanjs.git"
  },
  "license": "MIT",
  "dependencies": {
    "@rescript/core": "^1.6.1",
    "rescript": "^11.1.4",
    "vanjs-core": "^1.5.3"
  },
  "peerDependencies": {
    "vanjs-core": "^1.5.3"
  },
  "devDependencies": {
    "rescript-test": "^7.0.1",
    "vite": "^6.2.3"
  }
}
